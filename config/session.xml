<?xml version='1.0' encoding='ISO-8859-1'?>
<fix8>
   <default role="acceptor"
            fix_version="4400"
            target_comp_id="SPOT"
            heartbeat_interval="10"
            tcp_recv_buffer="100663296"
            tcp_send_buffer="100663296"
            tcp_nodelay="true"
            reset_sequence_numbers="false"
            ignore_logon_sequence_check="true"
            process_model="pipelined"
            enforce_compids="false"
            silent_disconnect="false"
            session_log="session_log"
            protocol_log="protocol_log"
            persist="file0"/>
 
   <session name="BROKER"
            ip="fix-oe.testnet.binance.vision"
            port="9000"
            active="true"
            sender_comp_id="BROKER" />
 
   <session name="FEEDER"
            ip="fix-md.testnet.binance.vision"
            port="9000"
            active="true"
            sender_comp_id="FEEDER" />
 
   <session name="DC"
            ip="fix-dc.testnet.binance.vision"
            port="9000"
            active="true"
            sender_comp_id="DC" />

    <persist name="file0"
        type="file" 
        dir="./run"
        use_session_id="true"
        rotation="5"
        db="client" />
  
    <log name="session_log"
        type="session"
        filename="|/bin/cat"
        rotation="5"
        delimiter="[]"
        levels="info|warn|error|fatal"
        flags="timestamp|sequence|compress|thread"/>
 
    <log name="protocol_log"
        type="protocol"
        filename="./run/myfix_client_protocol.log"
        rotation="5"
        flags="sequence|append|direction"/>

</fix8>